{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "next",
      "image": "qweghj1245/multi-next",
      "hostname": "next",
      "essential": false,
      "memory": 128
    },
    {
      "name": "nest",
      "image": "qweghj1245/multi-nest",
      "hostname": "nest",
      "essential": false,
      "memory": 128
    },
    {
      "name": "nginx",
      "image": "qweghj1245/multi-nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["next", "nest"],
      "memory": 128
    }
  ]
}